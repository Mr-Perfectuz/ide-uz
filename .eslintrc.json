{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_|^[A-Z]"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": "off"
  }
}